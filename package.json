{
  "name": "music-academy-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run clean-processes && npm run dev",
    "clean-processes": "powershell -ExecutionPolicy Bypass -File scripts/fix-websocket.ps1",
    "build": "vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:analyze": "npm run build && tsx scripts/analyze-bundle.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:montaje": "vitest --config vitest.config.montaje.ts",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=4096 eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,md}\" --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "validate": "npm run lint && npm run format -- --check && npm run type-check",
    "predeploy": "tsx src/utils/cli/migrate.ts",
    "deploy": "npm run build && firebase deploy",
    "permissions": "tsx scripts/setup-permissions.ts",
    "permissions:validate": "tsx scripts/validate-permissions.ts",
    "imports": "node scripts/advanced-import-resolver.js",
    "imports:analyze": "node scripts/safe-import-analyzer.js",
    "backup": "node scripts/backup-alumnos.js",
    "backup:list": "node -e \"import('./scripts/backup-alumnos.js').then(m => m.listAvailableBackups())\"",
    "migrate": "node scripts/safe-migration-process.js",
    "migrate:csv": "node scripts/migrate-students-from-csv.js",
    "standards": "node scripts/check-standards.js",
    "standards:show": "node -e \"import('./src/standards/development.standards.js').then(m => console.log(m.getFormattedStandards()))\""
  },
  "dependencies": {
    "@firebasegen/default-connector": "file:dataconnect-generated/js/default-connector",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.1",
    "@types/chart.js": "^2.9.41",
    "@types/file-saver": "^2.0.7",
    "@vuepic/vue-datepicker": "^7.4.0",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^10.7.2",
    "@vueuse/shared": "^10.7.2",
    "@whiskeysockets/baileys": "^6.7.18",
    "abcjs": "^6.3.0",
    "axios": "^1.6.7",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dompurify": "^3.2.4",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.2.0",
    "html2pdf.js": "^0.10.3",
    "idb": "^8.0.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lodash-es": "^4.17.21",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.4.1",
    "pinia": "^3.0.0",
    "pinia-plugin-persistedstate": "^4.3.0",
    "pino": "^9.7.0",
    "sortablejs": "^1.15.2",
    "tone": "^14.7.77",
    "vue": "^3.4.38",
    "vue-advanced-cropper": "^2.8.8",
    "vue-chartjs": "^5.3.2",
    "vue-draggable-plus": "^0.3.5",
    "vue-router": "^4.2.5",
    "vue-toast-notification": "^3.1.3",
    "vue-touch": "^2.0.0-beta.4",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5",
    "yup": "^1.6.1",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/papaparse": "^5.3.14",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.0.9",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "cspell": "^9.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-vue": "^9.20.1",
    "glob": "^11.0.3",
    "happy-dom": "^12.10.3",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "sass-embedded": "^1.89.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.2.2",
    "vite": "^6.2.3",
    "vite-plugin-pwa": "^0.21.2",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8",
    "workbox-background-sync": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run standards:check && npm run lint"
    }
  }
}
